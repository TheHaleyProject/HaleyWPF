    <Page Remove="Controls\FlexiLogIn\**" />
    <Page Remove="Controls\FlexiTab\**" />
  </ItemGroup>
	
  <ItemGroup>
    <Compile Remove="Controls\PlainDataGrid.cs" />
    <Compile Remove="Controls\PlainSlider.cs" />
    <Compile Remove="Internal\Controls\FlexiBorder.cs" />
  </ItemGroup>
	
  <ItemGroup>
    <None Remove="Properties\history.png" />
