<Window x:Class="WPF.Test.UITests.ContainerViewerTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF.Test.UITests"
        xmlns:test ="clr-namespace:WPF.Test.Controls"
        xmlns:hw="http://schemas.hpod9.com/haley/wpf"
        mc:Ignorable="d"
        Title="ContainerViewerTest" Height="450" Width="800">
    <Window.InputBindings>
        <KeyBinding Modifiers="Ctrl" Key="Q" Command="{Binding Cmd_changetheme}"/>
    </Window.InputBindings>
    <Grid>
        <hw:ContainerViewer ViewKey="{Binding Source={StaticResource GlobalData}, Path=CommonView,UpdateSourceTrigger=PropertyChanged}" Margin="20" ViewChanging="ContainerViewer_ViewChanging">
            <hw:ContainerViewer.WelcomeView>
                <test:WelcomeView/>
            </hw:ContainerViewer.WelcomeView>
        </hw:ContainerViewer>
    </Grid>
</Window>
