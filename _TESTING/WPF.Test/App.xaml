<Application x:Class="WPF.Test.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WPF.Test"
             xmlns:hw="http://schemas.hpod9.com/haley/wpf"
             xmlns:hm="http://schemas.hpod9.com/haley/mvvm"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <hm:CommonDictionary Source="pack://application:,,,/WPF.Test;component/Resources/DicRD.xaml"/>
                <hm:CommonDictionary Source="pack://application:,,,/Haley.WPF;component/Dictionaries/haleyRD.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/WPF.Test.External;component/Resources/RDMain.xaml"/>
                <!--<hw:CommonDictionary Source="pack://application:,,,/Haley.WPF.Fonts;component/Dictionaries/haleyFonts.xaml"/>-->
            </ResourceDictionary.MergedDictionaries>
            <local:VerificationConverter x:Key="verfiyConv"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
